{
  "presets": ["env", "react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "api": "./src/api",
          "assets": "./src/assets",
          "components": "./src/components",
          "store": "./src/store",
          "types": "./src/types",
          "utils": "./src/utils"
        }
      }
    ]
  ]
}
